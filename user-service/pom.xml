	<properties>
		<mysql-connector-j.version>.1.0</mysql-connector-j.version>
		<rest-assured.version>5.5.0</rest-assured.version>
		<datasource-proxy-spring-boot-starter.version>1.9.2</datasource-proxy-spring-boot-starter.version>
		<json-unit-assertj.version>3.4.1</json-unit-assertj.version>
		<commons-core.version>0.0.1-SNAPSHOT</commons-core.version>
