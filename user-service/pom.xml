		<relativePath/> <!-- lookup parent from repository -->
	</parent>

	<artifactId>user-service</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>User service</name>
	<description>Demo project for Spring Boot</description>
	<url/>
